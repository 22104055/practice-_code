#include <stdio.h

int main() {
    char a[100],b[100];
    scanf("%s %s",a,b);
    int i;
    for(i=0;a[i]!='\0';i++)
    {
        if(a[i]!=b[i])
        {
            if(a[i+1]!=b[i+1])
            {
            printf("%c",a[i]);
            }
            else
            {
                printf("%c,",a[i]);
            }
        }
        
    }
    for(i=0;b[i]!='\0';i++)
    {
        if(a[i]!=b[i])
        {
            if(a[i+1]!=b[i+1])
            {
            printf("%c",b[i]);
            }
            else
            {
                printf("%c,",b[i]);
            }
        }
        
    }

}
